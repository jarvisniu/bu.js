{
  "name": "bu.js",
  "version": "0.5.0-wip",
  "description": "A JavaScript 2D graphics library based on HTML5 Canvas",
  "main": "build/bu.js",
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c",
    "compile-example-lib": "npx coffee -c examples/lib",
    "fix": "eslint --fix src examples",
    "min": "uglifyjs build/bu.js -mco build/bu.min.js"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jarvisniu/bu.js.git"
  },
  "keywords": [
    "JavaScript",
    "2D",
    "graphics",
    "HTML5",
    "Canvas",
    "library",
    "geometry"
  ],
  "author": "Jarvis Niu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jarvisniu/bu.js/issues"
  },
  "homepage": "https://github.com/jarvisniu/bu.js#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "coffeescript": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.17.0",
    "rollup": "^0.52.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-coffee-script": "^2.0.0",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-serve": "^0.4.2",
    "uglify-js": "^3.2.2"
  }
}
