{
  "name": "bu.js",
  "version": "0.5.0-beta.1",
  "description": "A JavaScript 2D graphics library based on HTML5 Canvas",
  "main": "build/bu.js",
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c",
    "fix": "eslint --fix src examples"
  },
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jarvisniu/bu.js.git"
  },
  "keywords": [
    "HTML5",
    "Canvas",
    "2D",
    "graphics",
    "library"
  ],
  "author": "Jarvis Niu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jarvisniu/bu.js/issues"
  },
  "homepage": "https://github.com/jarvisniu/bu.js#readme",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.17.0",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-eslint": "^5.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-serve": "^1.0.1"
  },
  "eslintIgnore": [
    "build/**/*.js",
    "examples/**/*.html"
  ]
